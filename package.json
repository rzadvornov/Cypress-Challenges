{
  "name": "cypress",
  "version": "1.0.0",
  "description": "Cypress Challenges",
  "homepage": "https://github.com/rzadvornov/cypress#readme",
  "bugs": {
    "url": "https://github.com/rzadvornov/cypress/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rzadvornov/cypress.git"
  },
  "license": "ISC",
  "author": "Roman Zadvornov",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "clean-install": "rm -rf node_modules && npm install",
    "cy:open": "npx cypress open",
    "cy:run:chrome": "npx cypress run --browser chrome",
    "cy:run:firefox": "npx cypress run --browser firefox",
    "cy:test": "npx cypress run --spec 'cypress/e2e/features/*.feature'",
    "test": "cypress run",
    "test:parallel": "cypress run --record --parallel",
    "cy:test:mobile": "npx cypress run --config viewportWidth=375,viewportHeight=667"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.3.0",
    "@badeball/cypress-cucumber-preprocessor": "^23.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.5",
    "@eslint/js": "^9.30.1",
    "@faker-js/faker": "^9.9.0",
    "@percy/cli": "^1.31.1",
    "@percy/cypress": "^3.1.6",
    "cypress": "^14.5.4",
    "eslint": "^8.57.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {
    "eslint-config-airbnb-typescript": "^18.0.0",
    "http-method-enum": "^1.0.0",
    "status-code-enum": "^1.0.0"
  }
}
